{
  "name": "channel-forum",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc --project ./",
    "dev": "ts-node src/index.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "ENV=test ts-node ./node_modules/typeorm/cli -f ./src/config/ormconfig.ts migration:run && ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/config/ormconfig.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.43"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/supertest": "^2.0.11",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  }
}
